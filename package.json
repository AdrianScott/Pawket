{
  "name": "pawket",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn check-i18n --ci && yarn jest --ci && vue-cli-service build",
    "build-ext": "yarn check-i18n --ci && cross-env VUE_CLI_SERVICE_CONFIG_PATH=vue.config.ext.js vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serve-ext": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=vue.config.ext.js vue-cli-service --env.NODE_ENV=development build-watch --mode development",
    "check-i18n": "vue-i18n-extract report --vueFiles \"./src/**/*.?(js|ts|vue)\" --languageFiles \"./src/i18n/*.?(json|yml|yaml|js)\"",
    "test": "jest --coverage=false"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "@scure/base": "^1.0.0",
    "@types/sanitize-html": "^2.6.2",
    "@types/sortablejs": "^1.10.7",
    "bip39": "^3.0.4",
    "buefy": "^0.9.14",
    "buffer": "^6.0.3",
    "clvm_tools": "^0.9.5",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "js-big-decimal": "^1.3.4",
    "pbkdf2-hmac": "^1.0.4",
    "qrcode.vue": "^1.0.0",
    "register-service-worker": "^1.7.1",
    "sanitize-html": "^2.7.0",
    "sortablejs": "^1.14.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.27.0",
    "vue-i18n-extract": "^2.0.4",
    "vue-property-decorator": "^9.1.2",
    "vue-qrcode-reader": "^3.1.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.179",
    "@types/jest": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^28.1.1",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "~4.6.4",
    "vue-cli-plugin-chrome-extension-cli": "~1.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "webextensions": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
